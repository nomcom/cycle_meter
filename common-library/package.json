{
  "name": "common-library",
  "version": "1.0.0",
  "description": "backend&frontend 共通で使用するライブラリパッケージ",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "init": "run-s rest:client rest:doc",
    "lint": "eslint src --ext ts,tsx",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "rest:client": "orval",
    "rest:doc": "redocly build-docs openapi/openapi.yml --output ../doc/api/API仕様書.html",
    "rest:mock": "prism mock openapi/openapi.yml -p 3000 --cors"
  },
  "dependencies": {
    "@stoplight/prism-cli": "^4.10.6",
    "axios": "^1.3.4",
    "orval": "^6.12.1"
  },
  "devDependencies": {
    "@redocly/cli": "^1.0.0-beta.123",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "jest-mock-extended": "^3.0.1",
    "ts-jest": "^29.0.5",
    "eslint": "^8.34.0",
    "cross-env": "^7.0.3",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}